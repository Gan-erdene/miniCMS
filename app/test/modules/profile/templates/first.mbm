
<input type="hidden" name="submitted" value="1">
<ul class="profile_edit_ul">
    <li class="profile_edit_li">
        <p class="profile_edit_p">Ургийн овог:</p>
        <input type="text" placeholder="" class="urgiin_ovog" name="urgiin_ovog"/>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Овог:</p>
        <input type="text" placeholder="" class="ovog" name="ovog"/>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Нэр:</p>
        <input type="text" placeholder="" class="name" name="name"/>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Хүйс:</p>
        <label class="label_radio"><input type="radio" name="gender" value="1"/>Эрэгтэй</label>
        <label class="label_radio"><input type="radio" name="gender" value="0"/>Эмэгтэй</label>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Гэрлэлтийн байдал:</p>
        <label class="marry_label">                                        
            <div>
                <select name="relation">
                    <option value=""></option>
                    <option>Гэрэлсэн</option>                                                                              
                    <option>Сүй тавьсан</option>                                                                              
                    <option>Гэрлээгүй</option>                                                                              
                </select>
            </div>                                      
        </label>
        <br/>
        <br/>
        <label class="marry_label2">
            <input type="text" placeholder="tuya" class="who" name="hentei" name="who"/>
        </label>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Төрсөн огноо:</p>
        <div style="width:88px;float:left;margin-left:12px;margin-top:-1px;">
            <select name="year">
                <option value=""></option>
                {% for i in range(1985, 2010) %}
                    <option value="{{ i }}">{{ i }}</option>
                {% endfor %}                                          
            </select>
        </div>
        <div style="width:89px;float:left;margin-left:5px;margin-top:-1px;">
            <select name="mouth">
                <option value=""></option>
                {% for i in range(1, 12) %}
                    <option value="{{ i }}">{{ i }}</option>
                {% endfor %}
            </select>
        </div>
        <div style="width:89px;float:left;margin-left:5px;margin-top:-1px;">
            <select name="day">
                <option value=""></option>
                {% for i in range(1, 31) %}
                    <option value="{{ i }}">{{ i }}</option>
                {% endfor %}                                         
            </select>
        </div>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p"></p>
        <div style="width:250px;float:left;margin-left:12px;margin-top:-1px;">
            <select name="see">
                <option value=""></option>                                            
                <option value="all">Бүгдийг харуулах</option>
                <option value="year">Зөвхөн жилийг харуулах</option>
                <option value="month">Зөвхөн сарыг харуулах</option>
                <option value="day">Зөвхөн өдрийн харуулах</option>
            </select>
        </div>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Төрсөн газар:</p>
        <div style="width:137px;float:left;margin-left:12px;">
            <select name="birth_loc">
                <option value=""></option>
                <option value="mgl">Mongolia</option>
                <option value="us">United States</option>
            </select>
        </div>
        <div style="width:135px;float:left;margin-left:5px;">
            <select name="city">
                <option value=""></option>
                <option value="mgl">Mongolia</option>
            </select>
        </div>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Хэл:</p>
        <div style="width:277px;float:left;margin-left:12px;">
            <select>
                <option value=""></option>
                <option value="mgl">Mongolia</option>
            </select>
        </div>
    </li>
    <li class="profile_edit_li">
        <p class="profile_edit_p">Гэр бүл нэмэх:</p>
        <div style="width:137px;float:left;margin-left:12px;margin-top:-2px;">
            <select name="f_who">
                <option value=""></option>
                <option value="">Bat</option>
            </select>
        </div>
        <div style="width:135px;float:left;margin-left:5px;margin-top:-2px;">
            <select name="family">
                <option value=""></option>
                <option value="">Аав</option>
                <option value="">Ээж</option>
                <option value="">Ах</option>
                <option value="">Эгч</option>
                <option value="">Өвөө</option>
                <option value="">Эмээ</option>
                <option value="">Үеэл</option>
                <option value="">Зээ</option>
                <option value="">Ач</option>
            </select>
        </div>
        <a href="#" id="add_family"></a>
    </li>
</ul>
<div class="security_settings">
    <a href="#">Нууцлалын тохиргоо</a>
</div>
<!--button class="save">Өөрчлөлтийг хадгалах</button-->
<input type="submit" value="Өөрчлөлтийг хадгалах" class="uilchilge_noh_button">
<button class="cancel">Болих</button>
